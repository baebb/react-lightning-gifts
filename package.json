{
  "name": "react-lightning-gifts",
  "version": "0.5.2",
  "description": "Generate btc vouchers for use on the lightning network",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baebb/react-lightning-gifts.git"
