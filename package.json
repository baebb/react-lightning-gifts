    "react-ga": "~2.7.0",
    "react-helmet": "^5.2.0",
    "react-redux": "~7.1.3",
    "react-router": "~5.1.2",
    "react-router-dom": "~5.1.2",
    "redux": "~4.0.4",
    "redux-saga": "~1.1.3"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.3",
    "babel-eslint": "~10.0.3",
