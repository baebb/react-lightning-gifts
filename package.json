  "license": "MIT",
  "scripts": {
    "analyze": "NODE_ENV=size webpack-dev-server",
    "start": "NODE_ENV=test webpack-dev-server --open --history-api-fallback",
    "build:staging": "NODE_ENV=staging webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config webpack.config.js",
    "deploy:staging": "yarn run build:staging && firebase deploy --only hosting:development",
    "deploy:prod": "yarn run build:prod && firebase deploy --only hosting:production"
  },
